{
  "rules": {
    "indent": [ 2, 2 ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 1, "never" ],
    "yoda": [ 1, "always"],
    "eqeqeq": [ 1, "smart"],
    "curly": 0,
    "no-eval": 1,
    "no-debugger": 2,
    "no-use-before-define": [2, "nofunc"],
    "new-parens": 0,
    "no-underscore-dangle": 1,
    "no-unused-vars": [ 1, { "vars": "all", "args": "after-used" } ],
    "eol-last": 1
  },
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "extends": "eslint:recommended"
}
